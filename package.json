{
  "name": "librarian",
  "version": "0.0.0",
  "description": "The bookmark manager I always dreamed of",
  "author": "J Quinzell Rainwater",
  "license": "MIT",
  "scripts": {
    "start:front": "yarn --cwd frontend start",
    "start:back": "yarn --cwd backend start",
    "build:dev": "yarn run build:front && yarn run build:back",
    "build:prod": "yarn run build:front -- --package-json-only && yarn run build:back -- --package-json-only",
    "build:front": "yarn --cwd frontend install",
    "build:back": "yarn --cwd backend install",
    "watch:back": "yarn --cwd backend run watch",
    "run:front": "yarn --cwd frontend",
    "run:back": "yarn --cwd backend",
    "lint": "tslint -e ./**/*.test.js -e ./**/*.test.jsx -e ./**/*.test.tsx -e ./**/*.test.ts -p ./frontend && tslint -p ./backend"
  },
  "dependencies": {
    "@types/enzyme": "^3.9.1",
    "@types/jest": "^24.0.11",
    "enzyme": "^3.9.0",
    "jest": "^24.7.1",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  }
}