{
  "extends": "tslint:recommended",
  "rules": {
    "semicolon": [true, "never"],
    "quotemark": false,
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "object-literal-sort-keys": false,
    "only-arrow-functions": false
  }
}
