{
  "name": "Librarian",
  "browser_action": {
    "default_icon": "/images/icon.png",
    "default_title": "Librarian"
  },
  "page_action": {
    "browser_style": true,
    "show_matches": ["<all_urls>"],
    "default_icon": {
      "48": "/images/student.png"
    },
    "default_title": "bookmark page",
    "default_popup": "pageAction.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "48": "/images/icon.png"
  },
  "permissions": ["bookmarks", "tabs"],
  "manifest_version": 2,
  "version": "1"
}
